{
    "name": "vendor/quickbooks-connector",
    "description": "QuickBooks Web Connector integration for Magento 2",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Vendor",
            "email": "developer@example.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-soap": "*",
        "ext-xml": "*",
        "ext-json": "*",
        "ext-simplexml": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.10",
        "squizlabs/php_codesniffer": "^3.7",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-strict-rules": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Vendor\\QuickbooksConnector\\": "magento-module/Vendor/QuickbooksConnector/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vendor\\QuickbooksConnector\\Test\\": "magento-module/Vendor/QuickbooksConnector/Test/"
        }
    },
    "scripts": {
        "test": "phpunit -c magento-module/Vendor/QuickbooksConnector/Test/phpunit.xml.dist",
        "test-coverage": "phpunit -c magento-module/Vendor/QuickbooksConnector/Test/phpunit.xml.dist --coverage-html magento-module/Vendor/QuickbooksConnector/Test/coverage/html",
        "phpstan": "phpstan analyse --memory-limit=1G",
        "phpcs": "phpcs --standard=PSR12 magento-module/Vendor/QuickbooksConnector/Model magento-module/Vendor/QuickbooksConnector/Controller",
        "phpcbf": "phpcbf --standard=PSR12 magento-module/Vendor/QuickbooksConnector/Model magento-module/Vendor/QuickbooksConnector/Controller",
        "quality": [
            "@phpstan",
            "@phpcs"
        ],
        "ci": [
            "@test",
            "@quality"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
